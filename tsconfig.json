{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ESNext"],
    "module": "Node16",
    "moduleResolution": "Node16",
    "outDir": "dist",
    "skipLibCheck": true,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "noImplicitAny": false,
    "target": "es2016"
  },
  "exclude": ["node_modules", "dist"]
}
